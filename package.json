{
  "name": "test-apollo",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:start": "cd dist && PORT=8080 npx serve",
    "dev": "webpack serve --open --mode development",
    "start:live": "webpack serve --open --mode development --liveReload",
    "docker:build": "docker build . -t wp5-starter",
    "docker:run": "docker run -p 8080:8080 wp5-starter",
    "lint:ts": "tsc --noEmit",
    "lint:eslint": "eslint 'src/**/*.{js,ts,tsx}' --quiet",
    "lint:prettier": "prettier 'src/**/*.{js,ts,tsx}' -c",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:ts",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "7.13.15",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "7.13.13",
    "@babel/preset-typescript": "^7.18.6",
    "@linaria/webpack-loader": "^4.1.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-helmet": "^6.1.6",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@webpack-cli/serve": "^1.3.1",
    "babel-loader": "8.2.2",
    "css-loader": "^5.2.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.2",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "^2.8.4",
    "style-loader": "2.0.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "5.31.2",
    "webpack-cli": "4.6.0",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "@babel/runtime": "^7.13.10",
    "@linaria/babel-preset": "^4.3.3",
    "@linaria/core": "^4.2.5",
    "@linaria/react": "^4.3.3",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.11.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.6.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "bash -c tsc --noEmit",
      "eslint --fix --quiet",
      "prettier --write"
    ]
  }
}
